{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "import/extensions": ["never"],
    "rules": {
        "no-console": "off",
        "linebreak-style": "off"
    }
}
